    <div class="main-content">

        <div class="page-content">
            <div class="container-fluid">

                <!-- start page title -->
                <div class="row">
                    <div class="col-12">
                        <div class="page-title-box d-sm-flex align-items-center justify-content-between">
                            <h4 class="mb-sm-0">Vehicles</h4>

                            <div class="page-title-right">
                                <ol class="breadcrumb m-0">
                                    <li class="breadcrumb-item"><a href="javascript: void(0);">Dashboard</a></li>
                                    <li class="breadcrumb-item active">Add Vehicles</li>
                                </ol>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- end page title -->

                <div class="row">
                    <div class="col-12">
                        <div class="card">
                            <di class="card-body">
                                <br>
                                <h2 class="card-title">Add new vehicle</h4>
                                    <br>
                                    <form method="post" action="/add_vehicle">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="mb-3">
                                                    <label for="validationCustom01" class="form-label">Vehicle
                                                        name</label>
                                                    <input type="text" class="form-control" id="validationCustom01"
                                                        placeholder="Vehicle name" name="name" value="" required>
                                                    <div class="valid-feedback">
                                                        Looks good!
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="mb-3">
                                                    <label for="validationCustom02" class="form-label">Model
                                                        name</label>
                                                    <input type="text" class="form-control" id="validationCustom02"
                                                        placeholder="model name" name="model" value="" required>
                                                    <div class="valid-feedback">
                                                        Looks good!
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="validationCustom03" class="form-label">Category</label>
                                                    <select class="form-select" name="category" id="validationCustom03"
                                                        required>
                                                        <option value="">Choose</option>
                                                        <option value="Bike">Bike</option>
                                                        <option value="Car">Car</option>
                                                    </select>
                                                    <div class="invalid-feedback">
                                                        Please select a valid category.
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="validationCustom03" class="form-label">Breand</label>
                                                    <select class="form-select" name="brand" id="validationCustom03"
                                                        required>
                                                        <option value="">Choose</option>
                                                        <option value="TATA">TATA</option>
                                                        <option value="TVS">TVS</option>
                                                        <option value="Honda">Honda</option>
                                                        <option value="Hyundai">Hyundai</option>
                                                        <option value="Yamaha">Yamaha</option>
                                                        <option value="Hero">Hero</option>
                                                        <option value="Bajaj">Bajaj</option>
                                                        <option value="Royel Enfield">Royel Enfield</option>
                                                        <option value="Sujuki">Sujuki</option>
                                                    </select>
                                                    <div class="invalid-feedback">
                                                        Please select a valid Brand.
                                                    </div>

                                                </div>
                                            </div>

                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="validationCustom03" class="form-label">Body
                                                        type</label>
                                                    <select class="form-select" name="bodyType" id="validationCustom03"
                                                        required>
                                                        <option value="">Choose</option>
                                                        <option value="Motorcycles">Motorcycles</option>
                                                        <option value="Scooters">Scooters</option>
                                                        <option value="Hatchback">Hatchback</option>
                                                        <option value="Sedan">Sedan</option>
                                                        <option value="SUV">SUV</option>
                                                        <option value="Minivan">Minivan</option>
                                                        <option value="MUV">MUV</option>
                                                    </select>
                                                    <div class="invalid-feedback">
                                                        Please select a valid category.
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="validationCustom03" class="form-label">Fule
                                                        type</label>
                                                    <select class="form-select" name="fuelType" id="validationCustom03"
                                                        required>
                                                        <option value="">Choose</option>
                                                        <option value="Patrol">Patrol</option>
                                                        <option value="Diesel">Diesel</option>
                                                        <option value="Diesel">CNG</option>
                                                        <option value="EV">EV</option>
                                                    </select>
                                                    <div class="invalid-feedback">
                                                        Please select a valid category.
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="validationCustom02" class="form-label">Buying
                                                        Year</label>
                                                    <input type="number" name="buying" class="form-control"
                                                        id="validationCustom02" placeholder="Buying Year" value=""
                                                        required>
                                                    <div class="valid-feedback">
                                                        Looks good!
                                                    </div>

                                                </div>
                                            </div>

                                            <div class="col-md-4">
                                                <div class="mb-3">
                                                    <label for="validationCustom02" class="form-label">kilometre
                                                        Driven</label>
                                                    <input type="number" name="kms" class="form-control"
                                                        id="validationCustom02" placeholder="kilometre Driven" value=""
                                                        required>
                                                    <div class="valid-feedback">
                                                        Looks good!
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="mb-3">
                                                    <label for="validationCustom02" class="form-label">Price</label>
                                                    <input type="number" name="price" class="form-control"
                                                        id="validationCustom02" placeholder="Enter Price" value=""
                                                        required>
                                                    <div class="valid-feedback">
                                                        Looks good!
                                                    </div>

                                                </div>
                                            </div>

                                            <div class="col-md-6">
                                                <div class="mb-3">
                                                    <label for="validationCustom02" class="form-label">Primary
                                                        Image Url</label>
                                                    <div class="input-group">
                                                        <input type="text" name="image" class="form-control"
                                                            id="customFile" multiple>

                                                    </div>
                                                </div>
                                            </div>


                                            <div class="col-md-6">
                                                <div class="mb-3">
                                                    <label for="validationCustom02" class="form-label">Additional
                                                        Image Url</label>
                                                    <div class="input-group">
                                                        <input type="text" name="moreImages" placeholder="Image Url One"
                                                            class="form-control" multiple>
                                                        <button type="button"
                                                            class="btn btn-secondary addImageField">+</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <!-- Container to hold dynamically added image fields -->
                                        <div id="imageFieldsContainer"></div>
